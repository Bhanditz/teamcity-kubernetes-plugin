<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
       default-autowire="constructor">
    <bean id="kubeAgentConfigurationProvider" class="jetbrains.buildServer.clouds.kubernetes.KubeAgentConfigurationProvider" />
    <bean id="installBuildServiceFactory" class="jetbrains.buildServer.helm.InstallBuildServiceFactory" />
    <bean id="testBuildServiceFactory" class="jetbrains.buildServer.helm.TestBuildServiceFactory" />
    <bean id="deleteBuildServiceFactory" class="jetbrains.buildServer.helm.DeleteBuildServiceFactory" />
</beans>
